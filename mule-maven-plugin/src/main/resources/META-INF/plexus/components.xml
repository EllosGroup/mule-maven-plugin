<component-set>
    <components>
        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
            <role-hint>mule-application</role-hint>
            <implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping</implementation>
            <configuration>
                <lifecycles>
                    <lifecycle>
                        <id>default</id>
                        <phases>
                            <validate>
                                org.mule.tools.maven:mule-maven-plugin:validate
                            </validate>
                            <initialize>
                                org.mule.tools.maven:mule-maven-plugin:initialize
                            </initialize>
                            <generate-sources>
                                org.mule.tools.maven:mule-maven-plugin:generate-sources
                            </generate-sources>
                            <process-sources>
                                org.mule.tools.maven:mule-maven-plugin:process-sources
                            </process-sources>
                            <process-resources>
                                org.apache.maven.plugins:maven-resources-plugin:3.0.2:resources
                            </process-resources>
                            <compile>
                                org.apache.maven.plugins:maven-compiler-plugin:3.6.1:compile
                            </compile>
                            <generate-test-sources>
                                org.mule.tools.maven:mule-maven-plugin:generate-test-sources
                            </generate-test-sources>
                            <process-test-resources>
                                org.apache.maven.plugins:maven-resources-plugin:3.0.2:testResources
                            </process-test-resources>
                            <test-compile>
                                org.apache.maven.plugins:maven-compiler-plugin:3.6.1:testCompile
                            </test-compile>
                            <test>
                                org.apache.maven.plugins:maven-surefire-plugin:2.19.1:test
                            </test>
                            <package>
                                org.mule.tools.maven:mule-maven-plugin:package
                            </package>
                            <install>
                                org.apache.maven.plugins:maven-install-plugin:2.5.2:install
                            </install>
                            <deploy>
                                org.apache.maven.plugins:maven-deploy-plugin:2.8.2:deploy
                            </deploy>
                        </phases>
                    </lifecycle>
                </lifecycles>
            </configuration>
        </component>
        <component>
            <role>org.mule.tools.maven.plugin.app.cloudhub.CloudHubAdapter</role>
            <implementation>org.mule.tools.maven.plugin.app.cloudhub.DefaultCloudHubAdapter</implementation>
        </component>

        <!-- let maven plugins know that a mule project is a java project -->
        <component>
            <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
            <role-hint>mule-application</role-hint>
            <implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler</implementation>
            <configuration>
                <type>mule-application</type>
                <language>java</language>
                <addedToClasspath>true</addedToClasspath>
            </configuration>
        </component>




        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
            <role-hint>mule-policy</role-hint>
            <implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping</implementation>
            <configuration>
                <lifecycles>
                    <lifecycle>
                        <id>default</id>
                        <phases>
                            <validate>
                                org.mule.tools.maven:mule-maven-plugin:validate
                            </validate>
                            <initialize>
                                org.mule.tools.maven:mule-maven-plugin:initialize
                            </initialize>
                            <generate-sources>
                                org.mule.tools.maven:mule-maven-plugin:generate-sources
                            </generate-sources>
                            <process-sources>
                                org.mule.tools.maven:mule-maven-plugin:process-sources
                            </process-sources>
                            <process-resources>
                                org.apache.maven.plugins:maven-resources-plugin:3.0.2:resources
                            </process-resources>
                            <compile>
                                org.apache.maven.plugins:maven-compiler-plugin:3.6.1:compile
                            </compile>
                            <generate-test-sources>
                                org.mule.tools.maven:mule-maven-plugin:generate-test-sources
                            </generate-test-sources>
                            <process-test-resources>
                                org.apache.maven.plugins:maven-resources-plugin:3.0.2:testResources
                            </process-test-resources>
                            <test-compile>
                                org.apache.maven.plugins:maven-compiler-plugin:3.6.1:testCompile
                            </test-compile>
                            <test>
                                org.apache.maven.plugins:maven-surefire-plugin:2.19.1:test
                            </test>
                            <package>
                                org.mule.tools.maven:mule-maven-plugin:package
                            </package>
                            <install>
                                org.apache.maven.plugins:maven-install-plugin:2.5.2:install
                            </install>
                            <deploy>
                                org.apache.maven.plugins:maven-deploy-plugin:2.8.2:deploy
                            </deploy>
                        </phases>
                    </lifecycle>
                </lifecycles>
            </configuration>
        </component>

        <component>
            <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
            <role-hint>mule-policy</role-hint>
            <implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler</implementation>
            <configuration>
                <type>mule-policy</type>
                <language>java</language>
                <addedToClasspath>true</addedToClasspath>
            </configuration>
        </component>

    </components>

</component-set>

